{
  "name": "AGCP21",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "build": "tsc -p .",
    "build:live": "nodemon --watch 'src/**/*.ts' --exec \"ts-node\" src/server.ts",
    "start_": "npm run build:live",
    "start": "node --loader ts-node/esm src/server.ts",
    "test": "node --loader ts-node/esm src/test.ts",
    "start__": "ts-node src/server.ts --experimental-specifier-resolution=node",
    "test_": "echo \"Error: no test specified\" && exit 1",
    "addon": "node --experimental-specifier-resolution=node server.js",
    "ng": "ng",
    "start_ng": "ng serve",
    "build_ng": "ng build",
    "watch": "ng build --watch --configuration development",
    "build:elements": "ng build --configuration production --output-hashing none && node concatenate.js",
    "make": "ng build --configuration production --output-hashing none && node concatenate.js",
    "test_ng": "ng test"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^16.1.0",
    "@angular/common": "^16.1.0",
    "@angular/compiler": "^16.1.0",
    "@angular/core": "^16.1.0",
    "@angular/elements": "^16.1.8",
    "@angular/forms": "^16.1.0",
    "@angular/platform-browser": "^16.1.0",
    "@angular/platform-browser-dynamic": "^16.1.0",
    "@angular/router": "^16.1.0",
    "@avaya/neo": "^3.72.0",
    "@google-cloud/dialogflow-cx": "^3.8.0",
    "@google-cloud/discoveryengine": "^1.1.0",
    "@google-cloud/language": "^5.3.0",
    "@google-cloud/speech": "^6.0.2",
    "@google-cloud/text-to-speech": "^5.0.1",
    "@google-cloud/translate": "^7.2.2",
    "@google-cloud/vision": "^3.1.4",
    "google-auth-library": "^9.2.0",
    "readline": "^1.3.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.13.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^16.1.8",
    "@angular/cli": "~16.1.8",
    "@angular/compiler-cli": "^16.1.0",
    "@types/jasmine": "~4.3.0",
    "concat": "^1.0.3",
    "fs-extra": "^11.1.1",
    "jasmine-core": "~4.6.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "ts-node": "^10.9.2",
    "typescript": "~5.1.3"
  }
}
